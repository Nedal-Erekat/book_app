<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <main>
        <section>
           <% booksData.forEach(ele=>{  %>
            <div>
                <img src=" <%= ele.img %> " alt="">
                <h2> title: 
                    <%= ele.title %>
                </h2>
                <h3>Author: 
                    <%= ele.author %>
                </h3>
                <p>
                    <%= ele.description %>
                </p>
            </div>

           <% }) %>
        </section>
    </main>
</body>
</html>